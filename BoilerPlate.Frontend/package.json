	"scripts": {
		"dev": "ORIGIN=https://boilerplate.com PUBLIC_BASE_URL=https://boilerplate.com NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
		"build": "ORIGIN=https://boilerplate.com PUBLIC_BASE_URL=https://boilerplate.com vite build",
