{
	"name": "boilerplate-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "PUBLIC_BASE_URL=https://boilerplate.com NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
		"build": "ORIGIN=https://boilerplate.com PUBLIC_BASE_URL=https://boilerplate.com vite build",
		"build-prod": "ORIGIN=https://boilerplate.com PUBLIC_BASE_URL=http://ssr:5000 vite build",		
		"preview": "PUBLIC_BASE_URL=https://boilerplate.com vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
